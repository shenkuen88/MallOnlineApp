    <!--设置页面-->
    <style name="setLL">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/pxtodp95</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">@dimen/pxtosp22</item>
        <item name="android:background">@drawable/common_background</item>
    </style>

    <!--发布拼团-->
    <style name="publicTeamBuy">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/pxtodp95</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">@dimen/pxtosp30</item>
        <item name="android:paddingRight">@dimen/pxtosp30</item>
        <item name="android:background">@drawable/common_background</item>
    </style>

    <style name="setTV">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/title_black</item>
        <item name="android:textSize">@dimen/pxtosp30</item>
    </style>

    <style name="publicTeamBuyTvLeft">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/title_black</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/pxtosp32</item>
    </style>

    <style name="publicTeamBuyTvRight">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:drawablePadding">@dimen/pxtodp20</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/a9b</item>
        <item name="android:background">@color/white_color</item>
        <item name="android:layout_weight">2</item>
        <item name="android:textSize">@dimen/pxtosp30</item>
    </style>

    <style name="line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/pxtodp1</item>
        <item name="android:background">@color/ef</item>
    </style>
