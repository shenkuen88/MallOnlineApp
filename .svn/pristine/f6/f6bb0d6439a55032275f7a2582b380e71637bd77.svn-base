<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#404040"
    android:orientation="vertical">
    <ProgressBar
        android:id="@+id/progressbar_moredata"
        android:layout_width="@dimen/pxtodp60"
        android:layout_centerInParent="true"
        android:layout_height="@dimen/pxtodp60"
        android:indeterminate="true"
        android:indeterminateDrawable="@drawable/loading_progress_img"
        android:visibility="visible" />
    <android.support.v4.view.ViewPager
        android:id="@+id/viewpager"
        android:layout_marginBottom="@dimen/pxtodp80"
        android:layout_marginTop="@dimen/pxtodp70"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/common_back"></android.support.v4.view.ViewPager>

    <ImageView
        android:id="@+id/back_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/pxtodp24"
        android:src="@mipmap/hear_return_normal" />
    <TextView
        android:id="@+id/app_save_img"
        android:layout_width="wrap_content"
        android:textColor="@color/white_color"
        android:layout_alignParentBottom="true"
        android:paddingRight="@dimen/pxtodp18"
        android:paddingLeft="@dimen/pxtodp18"
        android:paddingTop="@dimen/pxtodp10"
        android:paddingBottom="@dimen/pxtodp10"
        android:layout_alignParentRight="true"
        android:layout_margin="@dimen/pxtodp24"
        android:background="@drawable/app_bn_white"
        android:layout_height="wrap_content"
        android:text="保存"/>
</RelativeLayout>
